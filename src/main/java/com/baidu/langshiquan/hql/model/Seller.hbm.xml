<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.baidu.langshiquan.hql.model.Seller" table="seller" schema="webshop">
        <id name="id" column="Id"/>
        <property name="name" column="name"/>
        <property name="tel" column="tel"/>
        <property name="address" column="address"/>
        <property name="website" column="website"/>
        <property name="star" column="star"/>
        <property name="business" column="business"/>
    </class>
</hibernate-mapping>